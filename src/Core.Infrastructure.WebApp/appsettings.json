{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "McpServerRepository": {
    "ConfigFilePath": "~/.mcp-servers/config.json"
  },
  "McpServerStatus": {
    "DefaultTimeZone": "Europe/Amsterdam"
  },
  "McpServerHosting": {
    "ToolInvocationTimeoutSeconds": 120,
    "ConnectionTimeoutSeconds": 30
  }
}
